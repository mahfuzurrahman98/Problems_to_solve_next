{
	"manifest_version": 3,
	"name": "Problems To Solve Next",
	"description": "an extenstion to store links of online judge problems",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html",
		"default_title": "Problems to solve next v1.0",
		"default_icon": {
			"16": "icon.png"
		}
	},
	"icons": {
		"16": "icon.png",
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": ["contextMenus", "tabs", "storage"],
	"background": {
		"service_worker": "./background.js"
	}
}
